<?xml version="1.0" encoding="utf-8"?>
<ripple
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#3E3737"
    >

    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <stroke
                        android:width="1dp"
                        android:color="#FFFFFF" />
                </shape>
            </item>
            <item android:state_pressed="false">
                <shape>
                    <stroke
                        android:width="1dp"
                        android:color="#FFFFFF" />
                </shape>
            </item>
        </selector>
    </item>


</ripple>






